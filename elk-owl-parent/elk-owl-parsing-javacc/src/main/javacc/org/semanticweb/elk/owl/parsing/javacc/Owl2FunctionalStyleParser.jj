{
	Vector<ElkDataPropertyExpression> dpList = new Vector<ElkDataPropertyExpression>();
	ElkDataRange dr;
	ElkDataSomeValuesFrom result = null;
}{
	<DATA_SOME_VALUES_FROM> <OPEN_BRACKET>
		addDataPropertyExpression(dpList)
		result = dataSomeValuesFromSuffix(dpList)
	{
		return result;
	}
}

ElkDataSomeValuesFrom dataSomeValuesFromSuffix(Vector<ElkDataPropertyExpression> dpList):
{
	ElkDataRange dr = null;
	ElkDataSomeValuesFrom result = null;
}{
	(
	LOOKAHEAD(3)
	dr = dataRange() <CLOSE_BRACKET>
	| 	(	addDataPropertyExpression(dpList)
			result = dataSomeValuesFromSuffix(dpList)
		) 
	)
	{
		return getElkObjectFactory().getDataSomeValuesFrom(dr, dpList);
	}
}


