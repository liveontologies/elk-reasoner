/* 9.5 Keys */
ElkHasKeyAxiom hasKey():
{
	ElkClassExpression ce = null;
	Set<ElkObjectPropertyExpression> opexprs = new HashSet<ElkObjectPropertyExpression>();
	Set<ElkDataPropertyExpression> dpexprs = new HashSet<ElkDataPropertyExpression>();
}{
	<HAS_KEY> <OPEN_BRACKET>
		axiomAnnotations()
		ce = classExpression()
		<OPEN_BRACKET> ( addObjectPropertyExpression(opexprs) )* <CLOSE_BRACKET>
		<OPEN_BRACKET> ( addDataPropertyExpression(dpexprs) )* <CLOSE_BRACKET>
	<CLOSE_BRACKET> {
		return getElkObjectFactory().getHasKeyAxiom(ce, opexprs, dpexprs);
	}
}

/* 9.6 Assertions */
ElkAssertionAxiom assertion():
{
	ElkAssertionAxiom x;
}{
	(
	x = sameIndividual()
	| x = differentIndividuals()
	| x = classAssertion()
	| x = objectPropertyAssertion()
	| x = negativeObjectPropertyAssertion()
	| x = dataPropertyAssertion()
	| x = negativeDataPropertyAssertion()
	) { return x; }
}

